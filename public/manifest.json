{
    "manifest_version": 3,
    "name": "Custom New Tab Page",
    "description": "A custom new tab page using Vue.js",
    "version": "1.0",
    "icons": {
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "permissions": ["storage"],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png"
        }
    }
}
